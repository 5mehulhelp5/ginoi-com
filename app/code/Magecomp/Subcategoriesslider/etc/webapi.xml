<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="POST" url="/V1/setsubcategoryslider">
        <service class="Magecomp\Subcategoriesslider\Api\PostManagementInterface" method="setSubcategorySlider"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="POST" url="/V1/setsubcategorylist">
        <service class="Magecomp\Subcategoriesslider\Api\PostManagementInterface" method="setSubcategoryList"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/setsubcategorylist/getConfigData">
        <service class="Magecomp\Subcategoriesslider\Api\PostManagementInterface" method="getConfigData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route> 
</routes>