<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Reports::report" title="Reports" sortOrder="70">
                    <resource id="Meetanshi_CustomCarrierTracking::custom_carrier" title="Meetanshi Custom Carrier Tracking" sortOrder="90">
                        <resource id="Meetanshi_CustomCarrierTracking::manage_custom_carrier" title="Manage Custom Carrier Trackings" sortOrder="20"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Meetanshi_CustomCarrierTracking::customcarriertracking" title="Meetanshi Custom Carrier Tracking" sortOrder="500"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
